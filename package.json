  "jest": {
    "testURL": "http://www.SomeAwesome.com",
    "collectCoverage": true,
    "coverageDirectory": "test/unit/coverage",
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/",
      "<rootDir>/node_modules/",
      "test/unit/coverage"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setup/jestTestSetup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css)$": "identity-obj-proxy",
      "^Sagas(.*)$": "<rootDir>/src/js/redux/sagas$1",
      "^APIs(.*)$": "<rootDir>/src/js/common/api$1",
      "^Assets(.*)$": "<rootDir>/src/assets$1",
      "^Components(.*)$": "<rootDir>/src/js/common/components$1",
      "^Constants(.*)$": "<rootDir>/src/js/common/constants$1",
      "^Redux(.*)$": "<rootDir>/src/js/redux/modules$1",
      "^Selectors(.*)$": "<rootDir>/src/js/redux/selectors$1",
      "^Services(.*)$": "<rootDir>/src/js/common/services$1",
      "^Themes(.*)$": "<rootDir>/src/js/common/themes$1",
      "^Types(.*)$": "<rootDir>/src/js/common/types$1",
      "^Utils(.*)$": "<rootDir>/src/js/common/utils$1"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "automock": false,
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx|json)$",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "globals": {
      "__CONFIG__": true,
      "GUI_VERSION": true
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test && npm run check-licenses"
    }
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "webpack-dev-server",
    "postcss",
    "jest",
    "boilerplate",
    "hmr"
  ]
}
